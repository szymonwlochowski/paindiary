%h2= link_to_unless_current post.title, post
= simple_format post.body
= post.pain_level
- post.descriptions.each do |d|
  %p= d.name
= link_to 'load as template', new_post_path(body: post.body, bodypart_id: post.bodypart_id, title: post.title, pain_level: post.pain_level, description_ids: post.description_ids)

